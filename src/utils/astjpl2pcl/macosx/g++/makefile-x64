$(OBJ_DIR)/astjpl2pcl: $(OBJ_FILES)
	clang++ -arch x86_64 -fPIC -headerpad_max_install_names -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX13.0.sdk -mmacosx-version-min=10.13 -stdlib=libc++ -Wl,-dead_strip -L"$(PCLLIBDIR64)" -L"/PixInsight/src/utils/cspice/lib" -o $(OBJ_DIR)/astjpl2pcl $(OBJ_FILES) -framework AppKit -framework ApplicationServices -lpthread -lPCL-pxi -llz4-pxi -lzlib-pxi -lRFC6234-pxi -llcms-pxi -lcminpack-pxi -lcspice -lcsupport
