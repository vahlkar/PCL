$(OBJ_DIR)/libzstd-pxi.a: $(OBJ_FILES)
	ar r $(OBJ_DIR)/libzstd-pxi.a $(OBJ_FILES)
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/libzstd-pxi.a

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/libzstd-pxi.a $(PCLLIBDIR64)

